<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.cxx.service.mapper.VisitorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.blog.cxx.service.entity.Visitor">
        <id column="id" property="id" />
        <result column="uuid" property="uuid" />
        <result column="ip" property="ip" />
        <result column="ip_source" property="ipSource" />
        <result column="os" property="os" />
        <result column="browser" property="browser" />
        <result column="pv" property="pv" />
        <result column="user_agent" property="userAgent" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uuid, ip, ip_source, os, browser, pv, user_agent, create_time, update_time
    </sql>

</mapper>
